{
  "options": {
    "name": {
      "name": "userName",
      "required": true,
      "pattern": "^(?![\\s-]*[\\s-])(?!.*--)(?!.*- )(?!.*'-)(?=.*[A-Za-zА-Яа-яЇїІіЄєҐґ])[A-Za-zА-Яа-яЇїІіЄєҐґ' -]+$",
      "minLength": 2,
      "maxLength": 70
    },
    "phone": {
      "name": "phoneNumber",
      "required": true,
      "pattern": "^\\+[0-9]+$",
      "minLength": 12,
      "maxLength": 13
    },
    "message": {
      "name": "userMessage",
      "required": false,
      "maxLength": 700
    },
    "agree": { "name": "userAgree", "required": true }
  }
}
